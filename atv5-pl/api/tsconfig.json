{
  "compilerOptions": {
    "target": "ES2017",
    "experimentalDecorators": true, 
    "emitDecoratorMetadata": true,
    "module": "CommonJS",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist", 
    "moduleResolution": "node",
    "noImplicitAny": false
  }
}
